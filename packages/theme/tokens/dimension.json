				}
			}
		},
		"base": {
			"$value": "{wpds-dimension.primitive.space.10}",
			"$description": "Base dimension unit"
		},
		"padding": {
			"xs": {
				"$value": "{wpds-dimension.primitive.space.10}",
				"$description": "Extra small padding",
				"$extensions": {
					"mode": {
						"compact": "{wpds-dimension.primitive.space.10}",
						"comfortable": "{wpds-dimension.primitive.space.20}"
					},
					"com.figma.scopes": [ "GAP" ]
				}
			},
			"sm": {
